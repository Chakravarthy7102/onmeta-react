{
  "name": "onmeta-react",
  "version": "0.1.5",
  "description": "Onmeta React SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "size": "size-limit",
    "publish": "node ./scripts/npm-publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chakravarthy7102/onmeta-react.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "onmeta",
    "react",
    "sdk"
  ],
  "author": "Chakravarthi Medicharla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chakravarthy7102/onmeta-react/issues"
  },
  "homepage": "https://github.com/Chakravarthy7102/onmeta-react#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@size-limit/esbuild": "^8.2.6",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^9.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.6",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "5.5 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "5 KB"
    }
  ],
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "goober": "^2.1.13"
  }
}