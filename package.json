{
	"name": "onmeta-react",
	"version": "1.0.0",
	"description": "Onmeta React SDK",
	"main": "index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"splitting": false,
		"sourcemap": true,
		"clean": true
	},
	"scripts": {
		"start": "tsup --watch",
		"build": "tsup"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Chakravarthy7102/onmeta-react.git"
	},
	"keywords": [
		"onmeta",
		"react",
		"sdk"
	],
	"author": "Chakravarthi Medicharla",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Chakravarthy7102/onmeta-react/issues"
	},
	"homepage": "https://github.com/Chakravarthy7102/onmeta-react#readme",
	"devDependencies": {
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"size-limit": "^8.2.6",
		"tsup": "^7.1.0",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"wagmi": "^1.3.8"
	},
	"size-limit": [
		{
			"path": "dist/index.js",
			"limit": "5.5 KB"
		},
		{
			"path": "dist/index.mjs",
			"limit": "5 KB"
		},
		{
			"path": "headless/index.js",
			"limit": "2 KB"
		},
		{
			"path": "headless/index.mjs",
			"limit": "2 KB"
		}
	]
}
